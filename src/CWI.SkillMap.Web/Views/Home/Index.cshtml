@model CWI.SkillMap.Web.ViewModels.Home.IndexViewModel
@{
    ViewBag.Title = "Home";
}

<div class="">
    <div class="row">
        <div class="col-md-2 text-center">
            <img height="144" src="~/profile-image/cwinet/alexandrelima" alt="..." class="img-rounded">
            <h4>Alexandre Machado</h4>
        </div>
        <div class="col-md-10">
            <div class="well bs-component">
                <fieldset>
                    <legend>Meus skills</legend>
                    <div>
                        @foreach (var item in Model.Skills)
                        {<div class="col-md-3">
                            <div class="panel panel-default mastery-10">
                                <div class="panel-body">
                                    @item.Name
                                </div>
                            </div>
                        </div>}
                    </div>
                </fieldset>
            </div>
        </div>
    </div>
</div>